`(,@(progn (push path callstack) nil)
  (ul ((class . "sidenav sidenav-fixed") (id . "sidenav"))
    (li ((class . "logo"))
      (a ((href . ,root-url))
        (img ((width . "250px")
              (src . "/leaf-browser/sources/splash.svg")))))
    (li ((class . "divider")))
    ,@(cond
       ((or homep groupp) (seml-import ".sidenav-group.seml"))))
  (div ((class . "patreon"))
    (a ((target . "_blank")
        (href . "https://www.patreon.com/conao3")) "Become a Patreon")))
